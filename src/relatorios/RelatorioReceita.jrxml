		<![CDATA[SELECT
     paciente.`idpaciente` AS paciente_idpaciente,
     paciente.`nome` AS paciente_nome,
     paciente.`telefone1` AS paciente_telefone1,
     paciente.`telefone2` AS paciente_telefone2,
